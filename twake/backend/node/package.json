    "fastify-cors": "^4.1.0",
    "fastify-formbody": "^5.0.0",
    "fastify-jwt": "^2.2.0",
    "fastify-multipart": "^4.0.3",
    "fastify-plugin": "^2.3.4",
    "fastify-sensible": "^3.0.1",
    "fastify-swagger": "^4.6.0",
    "fastify-websocket": "^2.0.11",
    "generate-password": "^1.6.0",
    "lodash": "^4.17.21",
    "match-all": "^1.2.6",
    "minio": "^7.0.18",
    "mongodb": "^4.0.0-beta.4",
    "multistream": "^4.1.0",
    "node-fetch": "^2.6.1",
    "node-uuid": "^1.4.8",
    "ora": "^5.3.0",
    "pino": "^6.8.0",
    "reflect-metadata": "^0.1.13",
    "resumable": "^0.0.1-1",
    "rxjs": "^6.6.3",
    "socket.io": "^2.3.0",
    "socket.io-redis": "^5.4.0",
    "socketio-jwt": "^4.6.2",
    "stream-to-iterator": "^3.0.3",
    "uuid": "^8.3.2",
